# API Configuration
NEXT_PUBLIC_API_URL=https://info1111final.vercel.app/api

# Authentication
NEXTAUTH_URL=https://info1111final.vercel.app
# Use a placeholder value that will be overridden by Vercel environment variables
NEXTAUTH_SECRET=placeholder_secret_value_for_production

# Database
# Use a placeholder value that will be overridden by Vercel environment variables
DATABASE_URL=postgresql://placeholder:placeholder@localhost:5432/placeholder

# Application Settings
NEXT_PUBLIC_APP_NAME=Property Maintenance System
NEXT_PUBLIC_APP_DESCRIPTION=Property maintenance request management system
NEXT_PUBLIC_CONTACT_EMAIL=support@example.com

# Feature Flags
NEXT_PUBLIC_ENABLE_ANALYTICS=true
NEXT_PUBLIC_ENABLE_MAINTENANCE_MODE=false
NEXT_PUBLIC_ENABLE_NOTIFICATIONS=true

# Maintenance Request Settings
NEXT_PUBLIC_MAX_REQUESTS_PER_PAGE=10
NEXT_PUBLIC_DEFAULT_PRIORITY=medium

# API Rate Limiting
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_WINDOW_MS=900000

# Security
SECURITY_HEADERS_ENABLED=true
CORS_ALLOWED_ORIGINS=https://info1111final.vercel.app
