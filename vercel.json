{
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "same-origin"
        },
        {
          "key": "Permissions-Policy",
          "value": "geolocation=(self), microphone=()"
        }
      ]
    }
  ],
  "regions": ["syd1"],
  "buildCommand": "npm run vercel-build",
  "installCommand": "npm install",
  "outputDirectory": ".next",
  "env": {
    "NEXT_PUBLIC_ENABLE_NOTIFICATIONS": "true",
    "NEXTAUTH_SECRET": "${NEXTAUTH_SECRET}",
    "NEXTAUTH_URL": "https://info1111-final.vercel.app",
    "NEXT_PUBLIC_SUPABASE_URL": "https://jsljlojidfkigqfaqscy.supabase.co",
    "NEXT_PUBLIC_SUPABASE_ANON_KEY": "${NEXT_PUBLIC_SUPABASE_ANON_KEY}",
    "NEXT_PUBLIC_ENABLE_ANALYTICS": "true",
    "NEXT_PUBLIC_ENABLE_MAINTENANCE_MODE": "false",
    "DIRECT_URL": "postgresql://postgres:Pepper@0566@db.jsljlojidfkigqfaqscy.supabase.co:5432/postgres"
  },
  "github": {
    "silent": true,
    "autoJobCancelation": true
  },
  "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.7.3",
      "memory": 1024,
      "maxDuration": 30
    },
    "src/app/api/**/*.ts": {
      "memory": 256,
      "maxDuration": 30,
      "excludeFiles": "node_modules/**",
      "includeFiles": "src/lib/supabase-client.ts"
    },
    "src/app/api/generate-report/*.ts": {
      "memory": 1024,
      "maxDuration": 60
    },
    "src/app/api/announcements/*.ts": {
      "memory": 512,
      "maxDuration": 30
    },
    "src/app/api/maintenance/*.ts": {
      "memory": 512,
      "maxDuration": 30
    },
    "src/app/api/notifications/*.ts": {
      "memory": 512,
      "maxDuration": 30
    }
  }
}
